    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "sort-package-json": "^2.1.0",
